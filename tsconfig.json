{
    "compilerOptions": {
      "module": "commonjs", // 编译生成的模块系统代码
      "target": "es2017", // 指定ecmascript的目标版本
      "noImplicitAny": false, // 禁止隐式any类型
      "outDir": "./out",
      "sourceMap": true,
      "allowJs": true, // 是否允许出现js
      "newLine": "LF",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
    },
    "include": ["src/**/*"]
  }